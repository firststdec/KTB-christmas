{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends":"airbnb",
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "windows"],
    "object-curly-newline": "off",
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "spaced-comment": "off",
    "prefer-arrow-callback": "off",
    "quote-props": "off",
    "no-new": "off",
    "arrow-parens": [2, "as-needed"],
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off"
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "settings": {
    "react": {
      "version": "16.8"
    }
  }
}
